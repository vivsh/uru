{
  "name": "uru",
  "version": "0.3.6",
  "description": "virtual-dom library",
  "main": "uru/index.js",
  "scripts": {
    "test": "node test/nodes && node test/component",
    "watch": "webpack --watch --output-library uru --output-library-target umd uru/index.js dist/uru.js",
    "build-lib": "webpack --output-library uru --output-library-target umd uru/index.js dist/uru.js",
    "build-lib-min": "webpack --output-library uru --optimize-minimize --output-library-target umd uru/index.js dist/uru.min.js",
    "build": "npm run build-lib && npm run build-lib-min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://vivsh@github.com/vivsh/uru.git"
  },
  "keywords": [
    "virtual-dom",
    "uru",
    "javascript",
    "dom"
  ],
